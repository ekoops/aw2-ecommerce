  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/eureka-js-client": "^4.5.3",
    "@types/express": "^4.17.13",
    "@types/express-validator": "^3.0.0",
    "@types/kafkajs": "^1.9.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
